#!/bin/sh
echo "script started"
sudo modprobe rma

for i in 65 70 75 80 85 90 95 100 105 110 115 120 125 130 135 140 145 150 155 160 165 170 175 180 185 190 195 200 205 210 215 220 225 230 235 240 245 250
do
echo ""
echo "20t_nl $i"
sudo sched_test_app -s RMA -f chronos/ChronOS_3.0/Task\ Sets/20t_nl -c $i -r 20
echo "27t_nl $i"
sudo sched_test_app -s RMA -f chronos/ChronOS_3.0/Task\ Sets/27t_nl -c $i -r 20
done

sudo rmmod rma

echo "~~~~~~ starting EDF~~~~~~~"

sudo modprobe myedf
for i in 65 70 75 80 85 90 95 100 105 110 115 120 125 130 135 140 145 150 155 160 165 170 175 180 185 190 195 200 205 210 215 220 225 230 235 240 245 250
do
echo ""
echo "20t_nl $i"
sudo sched_test_app -s MYEDF -f chronos/ChronOS_3.0/Task\ Sets/20t_nl -c $i -r 20
echo "27t_nl $i"
sudo sched_test_app -s MYEDF -f chronos/ChronOS_3.0/Task\ Sets/27t_nl -c $i -r 20
done
sudo rmmod myedf


echo "~~~~~~ starting HVDF ~~~~~"

sudo modprobe hvdf
for i in 65 70 75 80 85 90 95 100 105 110 115 120 125 130 135 140 145 150 155 160 165 170 175 180 185 190 195 200 205 210 215 220 225 230 235 240 245 250
do
echo ""
echo "20t_nl $i"
sudo sched_test_app -s HVDF -f chronos/ChronOS_3.0/Task\ Sets/20t_nl -c $i -r 20
echo "27t_nl $i"
sudo sched_test_app -s HVDF -f chronos/ChronOS_3.0/Task\ Sets/27t_nl -c $i -r 20
done
sudo rmmod hvdf

echo "sript complete"
